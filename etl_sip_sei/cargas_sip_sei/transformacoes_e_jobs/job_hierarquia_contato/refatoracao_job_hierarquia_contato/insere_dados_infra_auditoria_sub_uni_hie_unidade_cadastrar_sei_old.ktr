    <sql>select uni.id_unidade  as id_unidade_req,
	   concat('GET - Array' + CHAR(13)+CHAR(10) + '('
	   + CHAR(13)+CHAR(10) + ')' + CHAR(13)+CHAR(10)
	   + 'POST - Array' + CHAR(13)+CHAR(10) + '(' + CHAR(13)+CHAR(10)
       + ')' + CHAR(13)+CHAR(10), null) as requisicao_formatada,
	   
	   concat('UnidadeRN::cadastrarRN0078Controlado(' + CHAR(13)+CHAR(10) 
		+ 'UnidadeDTO:' + CHAR(13)+CHAR(10) 
		+ 'IdUnidade =', ' ' + cast(uni.id_unidade as varchar(10)) + CHAR(13)+CHAR(10)
		+ 'IdOrigem = [null]' + CHAR(13)+CHAR(10)
		+ 'IdOrgao =', ' ', cast(uni.id_orgao as varchar(10)) + CHAR(13)+CHAR(10) 
		+ 'Sigla =', ' ' + uni.sigla + CHAR(13)+CHAR(10) 
        + 'Sigla =', ' ' + uni.descricao + CHAR(13)+CHAR(10) 
		+ 'SinAtivo = S' + ')') as operacao_formatada,
   	   		 
	   rel_hie.id_unidade,
	   rel_hie.id_hierarquia_pai as id_hie_pai,
	   rel_hie.id_unidade_pai as id_uni_pai	

from unidade uni
JOIN rel_hierarquia_unidade rel_hie ON uni.id_unidade = rel_hie.id_unidade
where rel_hie.id_unidade_pai is not null 
	and rel_hie.id_hierarquia_pai is not null
	and MONTH(dta_inicio) = MONTH(dateadd(dd, -1, GetDate()))
	and YEAR(dta_inicio) = YEAR(dateadd(dd, -1, GetDate()))
    and dta_inicio > DateADD(mi, -10, Current_TimeStamp)
    order by uni.id_unidade asc
</sql>
